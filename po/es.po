msgid_plural "%d bulls, "
msgstr[0] "¡%d toro, "
msgstr[1] "¡%d toros, "

#: ../src/gnome-mastermind.c:1035
#, c-format
msgid "%d cow!"
msgid_plural "%d cows!"
msgstr[0] "%d vaca!"
msgstr[1] "%d vacas!"

