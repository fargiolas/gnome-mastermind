msgid "%d bull, "
msgid_plural "%d bulls, "
msgstr[0] "%d taureau, "
msgstr[1] "%d taureaux, "

#: ../src/gnome-mastermind.c:1035
#, c-format
msgid "%d cow!"
msgid_plural "%d cows!"
msgstr[0] "%d vache !"
msgstr[1] "%d vaches !"

