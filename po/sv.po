msgid_plural "%d bulls, "
msgstr[0] "%d vit, "
msgstr[1] "%d vita, "

#: ../src/gnome-mastermind.c:1035
#, c-format
msgid "%d cow!"
msgid_plural "%d cows!"
msgstr[0] "%d svart!"
msgstr[1] "%d svarta!"

